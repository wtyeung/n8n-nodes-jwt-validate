{
    "name": "n8n-nodes-jwt-validate",
    "version": "0.1.1",
    "description": "n8n node for JWT token validation with JWKS support",
    "license": "MIT",
    "homepage": "https://github.com/wtyeung/n8n-nodes-jwt-validate#readme",
    "keywords": [
        "n8n-community-node-package",
        "jwt",
        "validation",
        "jwks",
        "authentication",
        "security"
    ],
    "author": {
        "name": "Tim Yeung",
        "email": "tim.yeung@hku.hk"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wtyeung/n8n-nodes-jwt-validate.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [],
        "nodes": [
            "dist/nodes/Example/Example.node.js",
            "dist/nodes/JwtValidate/JwtValidate.node.js"
        ],
        "strict": false
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@types/jsonwebtoken": "^9.0.10",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "dependencies": {
        "jsonwebtoken": "^9.0.2",
        "jwks-rsa": "^3.1.0"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
